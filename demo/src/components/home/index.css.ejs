<% if (style === 'less') { %>
:global {
  .ant-modal-content {
    height: 750px;
    width: 800px;
    background-color: #212538 !important;
    .ant-modal-body {
      height: 80%;
    }
  }
  #board-content-wrapper {
    .common-background-color {
      // background-color: #212538;
      background-color: #fff;
      box-shadow: 0px 0px 20px 2px aliceblue;
      transition: all 0.5;
      overflow: hidden;
    }
    .common-background-color:hover {
      box-shadow: 0px 0px 20px 0px #888888;
    }
    display: flex;
    height: 100vh;
    width: 100%;
    .board-content-left {
      .ant-menu {
        height: 100%;
        padding-top: 30px;
        background-color: #080e1c;
        & > img {
          width: 90%;
          margin-left: 5px;
          margin-top: -5px;
        }
        & > li:nth-of-type(1) {
          margin-top: 30px;
        }
      }
    }
    .board-content-right {
      flex: 1;
      overflow: auto;
      .content-header {
        display: flex;
        justify-content: flex-start;
        align-items: center;
        height: 38px;
        background-color: #00152aeb;
        .header-user-logo {
          & > i {
            margin: 0 10px 0 10px;
            color: #fff;
          }
        }
        .header-user-info {
          color: #fff;
          font-size: 14px;
        }
      }
      .content-substance {
        display: flex;
        height: 30%;
        width: 95%;
        margin: auto;
        .substance-recommend {
          flex: 65%;
          display: flex;
          padding: 15px;
          .recommend-1 {
            flex: 1;
            margin: 10px;
            .recommend-1-content {
              height: 100%;
            }
          }
          .recommend-2 {
            flex: 1;
            margin: 10px;
          }
        }
        .substance-high-quality {
          flex: 35%;
          display: flex;
          flex-wrap: wrap;
          justify-content: space-between;
          padding: 15px;
          .high-quality-wrapper {
            flex: 1;
            width: calc(50% - 10px);
            min-width: calc(50% - 10px);
            max-width: calc(50% - 10px);
            height: calc(50% - 10px);
            margin: 5px;
            .high-quality-content {
              position: relative;
              height: 100%;
              & > div {
                position: absolute;
                top: 50%;
                width: 100%;
                transform: translateY(-50%);
              }
            }
          }
        }
      }
      .content-ranking {
        display: flex;
        height: 30%;
        width: 95%;
        margin: auto;
        .ranking-1 {
          flex: 65%;
          padding: 15px;
          .ranking-content-1 {
            height: calc(100% - 20px);
            width: calc(100% - 20px);
            margin: 10px;
            & > div {
              height: 100%;
            }
          }
        }
        .ranking-2 {
          flex: 35%;
          padding: 15px;
          .ranking-content-2 {
            height: calc(100% - 20px);
            width: calc(100% - 20px);
            margin: 10px;
          }
        }
      }
      .content-downloads {
        height: calc(40% - 38px);
        padding: 10px;
        width: 95%;
        margin: auto;
        padding: 15px;
        .downloads-content {
          height: 100%;
          width: 100%;
        }
      }
    }
  }
}
    
  
  
    
